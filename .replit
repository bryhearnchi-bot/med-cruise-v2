modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"

[nix]
channel = "stable-25_05"

[deployment]
run = ["npm", "start"]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37905
externalPort = 3000

[[ports]]
localPort = 39051
externalPort = 3003

[[ports]]
localPort = 41751
